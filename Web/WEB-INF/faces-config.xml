<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">


	<!-- Arquivo de Mensagem. Mensagem.properties -->
	<application>
		<message-bundle>constantes</message-bundle>
	</application>


	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	
	<converter>
		<converter-id>OrgaoConverter</converter-id>
		<converter-class>br.com.sisfie.converter.OrgaoConverter</converter-class>
	</converter>
	
	<converter>
		<converter-id>CPFConverter</converter-id>
		<converter-class>br.com.sisfie.converter.CPFConverter</converter-class>
	</converter>
</faces-config>
